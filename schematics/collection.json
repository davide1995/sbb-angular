{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "documentation": {
      "description": "Generate documentation for the showcase.",
      "factory": "./documentation/index#documentation"
    },
    "example-migration": {
      "description": "Mirate examples",
      "factory": "./example-migration/index#exampleMigration"
    },
    "public2business": {
      "description": "Copy public components to business",
      "factory": "./public2business/index#public2business"
    },
    "generate-icon-modules": {
      "description": "Generate icon modules.",
      "factory": "./generate-icon-modules/index#generateIconModules"
    },
    "init-svg-registry": {
      "description": "Generate an svg registry, from which icon modules can be generated.",
      "factory": "./init-svg-registry/index#initSvgRegistry",
      "schema": "./init-svg-registry/schema.json"
    },
    "generate-from-svg-registry": {
      "description": "Generate icon modules.",
      "factory": "./generate-from-svg-registry/index#generateFromSvgRegistry",
      "schema": "./generate-from-svg-registry/schema.json"
    }
  }
}
